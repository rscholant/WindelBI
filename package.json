{
  "name": "windel-bi",
  "version": "1.0.1",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start:dev": "ts-node-dev -r tsconfig-paths/register --transpile-only --ignore-watch node_modules --no-notify src/index.ts",
    "lint": "eslint --fix './src/**/*.ts'",
    "ident": "prettier --write --ignore-unknown './**/*.ts' ",
    "build": "tsc",
    "pkg:windows": "pkg . --target=node14-win-x64 --output windel-bi.exe",
    "pkg:linux": "pkg . --target=latest-linux-x64 --output windel-bi.out"
  },
  "devDependencies": {
    "@types/md5": "^2.3.0",
    "@types/node": "^15.0.2",
    "@typescript-eslint/eslint-plugin": "^4.22.1",
    "@typescript-eslint/parser": "^4.22.1",
    "eslint": "^7.25.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.4.0",
    "husky": "^6.0.0",
    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1",
    "ts-node-dev": "^1.1.6",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "@types/socket.io": "^2.1.13",
    "@types/ws": "^7.4.2",
    "axios": "^0.21.1",
    "date-fns": "^2.21.2",
    "dotenv": "^8.5.1",
    "md5": "^2.3.0",
    "node-firebird": "^0.9.9",
    "socket.io": "^4.0.2",
    "socket.io-client": "^4.0.2",
    "winston": "^3.3.3",
    "ws": "^7.4.5"
  },
  "lint-staged": {
    "**/*": [
      "eslint --fix './src/**/*.ts'",
      "prettier --write --ignore-unknown './**/*.ts' ",
      "git add"
    ]
  }
}
